{
    "name": "smpladminui",
    "private": false,
    "version": "1.1.12",
    "keywords": [],
    "description": "SimpleAdmin UI React Components Library",
    "author": "marDonchev",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
        "lint:fix": "eslint --fix 'src/**/*.{jsx,ts,tsx}'",
        "format": "prettier --write src//**/*.{ts,tsx,css} --config ./.prettierrc",
        "prepare": "husky install",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx}": [
            "yarn  eslint",
            "yarn  format"
        ]
    },
    "files": [
        "dist/*"
    ],
    "exports": {
        ".": {
            "import": "./dist/smpladminui.js",
            "require": "./dist/smpladminui.umd.cjs"
        },
        "./dist/styles.css": "./dist/styles.css"
    },
    "main": "./dist/smpladminui.umd.cjs",
    "module": "./dist/smpladminui.js",
    "types": "./dist/index.d.ts",
    "devDependencies": {
        "@babel/core": "^7.20.12",
        "@storybook/addon-actions": "^6.5.16",
        "@storybook/addon-essentials": "^6.5.16",
        "@storybook/addon-interactions": "^6.5.16",
        "@storybook/addon-links": "^6.5.16",
        "@storybook/builder-vite": "^0.4.0",
        "@storybook/react": "^6.5.16",
        "@storybook/testing-library": "^0.0.13",
        "@types/node": "^18.7.6",
        "@types/react": "^18.0.17",
        "@types/react-dom": "^18.0.6",
        "@typescript-eslint/eslint-plugin": "^5.34.0",
        "@typescript-eslint/parser": "^5.34.0",
        "@vitejs/plugin-react": "^2.1.0",
        "babel-loader": "^8.3.0",
        "eslint": "^8.33.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "eslint-plugin-storybook": "^0.6.10",
        "husky": "^8.0.1",
        "lint-staged": "^13.0.3",
        "prettier": "^2.7.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rollup-plugin-css-only": "^4.3.0",
        "sass": "^1.62.1",
        "typescript": "^4.6.4",
        "vite": "^3.1.0",
        "vite-plugin-dts": "^1.4.1",
        "vite-plugin-linter": "^1.2.0",
        "vite-tsconfig-paths": "^3.5.0"
    },
    "peerDependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/marDonchev/smpladminui-vite.git"
    },
    "bugs": {
        "url": "https://github.com/marDonchev/smpladminui-vite/issues"
    },
    "homepage": "https://github.com/marDonchev/smpladminui-vite#readme"
}
